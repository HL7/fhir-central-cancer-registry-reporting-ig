        </telecom>
    </contact>
    <description value="This profile represents the encounter related to the cancer event. It is based on US Core Encounter."/>
    <jurisdiction>
        <coding>
            <system value="urn:iso:std:iso:3166"/>
            <code value="US"/>
            <display value="United States of America"/>
        </coding>
        <text value="United States of America"/>
    </jurisdiction>
    <fhirVersion value="4.0.1"/>
    <kind value="resource"/>
    <abstract value="false"/>
    <type value="Encounter"/>
    <baseDefinition value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-encounter|6.1.0"/>
