    <differential>
        <element id="Encounter">
            <path value="Encounter"/>
            <short value="Central Cancer Registry Reporting Encounter"/>
            <definition value="This profile is based on US Core and establishes the core elements, extensions, vocabularies and value sets for central cancer registry reporting encounters."/>
        </element>
        <element id="Encounter.subject">
            <path value="Encounter.subject"/>
            <min value="1"/>
            <type>
                <code value="Reference"/>
                <targetProfile value="http://hl7.org/fhir/us/central-cancer-registry-reporting/StructureDefinition/central-cancer-registry-reporting-patient"/>
            </type>
            <mustSupport value="true"/>
        </element>
        <element id="Encounter.participant">
            <path value="Encounter.participant"/>
            <slicing>
                <discriminator>
                    <type value="type"/>
                    <path value="type"/>
                </discriminator>
                <rules value="open"/>
            </slicing>
            <definition value="The provider in charge of care and the facility in which care was provided during the encounter"/>
            <mustSupport value="true"/>
        </element>
        <element id="Encounter.participant:sliceResponsibleProvider">
            <path value="Encounter.participant"/>
            <sliceName value="sliceResponsibleProvider"/>
            <short value="Responsible provider"/>
            <definition value="The provider in charge of care and the facility in which care was provided during the encounter"/>
            <max value="1"/>
            <mustSupport value="true"/>
        </element>
        <element id="Encounter.participant:sliceResponsibleProvider.type">
            <path value="Encounter.participant.type"/>
            <short value="Responsible provider"/>
            <min value="1"/>
            <max value="1"/>
            <patternCodeableConcept>
                <coding>
                    <system value="http://terminology.hl7.org/CodeSystem/v3-ParticipationType"/>
                    <code value="ATND"/>
                </coding>
            </patternCodeableConcept>
            <mustSupport value="true"/>
        </element>
        <element id="Encounter.participant.period">
            <path value="Encounter.participant.period"/>
            <mustSupport value="true"/>
        </element>
        <element id="Encounter.participant:sliceResponsibleProvider.individual">
            <path value="Encounter.participant.individual"/>
            <short value="Responsible Provider"/>
            <definition value="The provider in charge of care and the facility in which care was provided during the encounter"/>
            <type>
                <code value="Reference"/>
                <targetProfile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-practitioner|6.1.0"/>
            </type>
            <mustSupport value="true"/>
        </element>
        <element id="Encounter.reasonReference">
            <path value="Encounter.reasonReference"/>
			<mustSupport value="true"/>
			<type>
				<code value="Reference"/>
				<targetProfile value="http://hl7.org/fhir/us/central-cancer-registry-reporting/StructureDefinition/central-cancer-registry-primary-cancer-condition"/>
				<targetProfile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-condition-problems-health-concerns|6.1.0"/>
				<targetProfile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-condition-encounter-diagnosis|6.1.0"/>
				<targetProfile value="http://hl7.org/fhir/us/core/StructureDefinition/us-core-procedure|6.1.0"/>
				<targetProfile value="http://hl7.org/fhir/StructureDefinition/Observation"/>
				<targetProfile value="http://hl7.org/fhir/StructureDefinition/ImmunizationRecommendation"/>			
			</type>
		</element>
